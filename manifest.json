{
  "name": "FFFFOUND",
  "version": "2.0.5",
  "description": "Adds an option to add images on a page to FFFFOUND.",
  "icons": { 
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },

  "browser_action": {
    "default_icon": "favicon.ico",
    "default_title": "Add to FFFFOUND"
  },

  "background_page": "background.html",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ]  
}